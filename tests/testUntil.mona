  pred system2(var2 Mot, Pre0, Pre1, P, Q, Post0, Post1) =
    P = {1,2,3,5} &
    Q = {6} &
    Mot = {0,1,2,3,4,5,6} &
    Pre0 = {2,5,6} &
    Pre1 = {2,3,4,5} &
