    Pre0 = {1,3,4} &
    Pre1 = {2,3} &
    Post0 = {0,1} &
    Post1 = {0,2}
  ;


  var2 Mot;
  var2 Pre0, Pre1;
  var2 Post0, Post1;

  var2 P;
  var2 Q;
  var2 EX;
  var2 AX;
  var2 PEUQ;
  var2 PAUQ;
  var2 PGUQ;

  system2( Mot, Pre0, Pre1, P, Q, Post0, Post1);
  EXP(EX,P, Pre0,Pre1,Post0, Post1, Mot);
  AXP(AX,P, Pre0,Pre1,Post0, Post1, Mot);
  pEUq(PEUQ,P,Q,Pre0,Pre1,Post0, Post1, Mot);
  pAUq(PAUQ,P,Q,Pre0,Pre1,Post0, Post1, Mot);
  pGUq(PGUQ,P,Q,Pre0,Pre1,Post0, Post1, Mot);
