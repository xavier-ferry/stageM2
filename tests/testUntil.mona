  pred system(var2 Mot, Pre0, Pre1, P, Q, Post0, Post1) =
    P = {1,2,4} &
    Q = {4} &
    Mot = {0,1,2,3,4} &
    Pre0 = {2,4} &
    Pre1 = {2,3,4} &
    Post0 = {0,2} &
    Post1 = {1,3}
